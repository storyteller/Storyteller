{
  "buildOptions": {
    "outputName": "Storyteller",
    "debugType": "portable",
    "emitEntryPoint": false,
    "compile": [
      "**/*.cs",
      "../CommonAssemblyInfo.cs"
    ],
    "embed": [ "../../client/public/**/*.css", "../StorytellerRunner/batch-bundle.js" ]
  },
  "version": "4.0.0-alpha-*",
  "title": "Storyteller",
  "description": "Acceptance Test Driven Development and Executable Specifications for .Net",
  "language": "en-US",
  "authors": [
    "Jeremy D. Miller"
  ],
  "packOptions": {
    "projectUrl": "http://storyteller.github.io",
    "tags": [
      "testing", "storyteller", "specifications", "bdd", "atdd"
    ],
    "licenseUrl": "https://github.com/storyteller/storyteller/raw/master/LICENSE.TXT",
    "repository": {
      "type": "git",
      "url": "git://github.com/storyteller/storyteller"
    },
    "iconUrl": "http://storyteller.github.io/storyteller/content/images/favicon.ico"
  },
  "dependencies": {
    "Baseline": "1.1.0",
    "Newtonsoft.Json": "9.0.1",
    "System.Reflection.TypeExtensions": "4.1.0",
    "Microsoft.DotNet.InternalAbstractions": "1.0.0" 
  },
  "frameworks": {
    "net46": {
      "frameworkAssemblies": {
        "System.Data": "",
        "System.Reflection": "",
        "System.Web":  "" 
      },
      "dependencies": {
        "System.Data.Common": "4.1.0"

      }
    },
    "netstandard1.6": {
      "dependencies": {
        "System.Data.Common": "4.1.0",
        "System.Xml.XmlDocument": "4.0.1",
        "System.Net.NameResolution": "4.0.0",
        "System.Net.Sockets": "4.1.0",
        "System.Diagnostics.TextWriterTraceListener": "4.0.0",
        "System.Diagnostics.Debug": "4.3.0",
        "System.Threading.Thread": "4.0.0",
        "System.Threading.Timer": "4.0.1",
        "System.Threading": "4.3.0",
        "System.Text.Encodings.Web": "4.3.0",
        "System.Console": "4.3.0",
        "Microsoft.Extensions.DependencyModel": "1.1.0" 
      }
    }
  }
}
